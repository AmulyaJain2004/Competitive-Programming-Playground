#include <bits/stdc++.h>using namespace std; int main() {    int t;    cin >> t;    while (t--) {        int n, k;        cin >> n >> k;        // Just if k exists in array or not will solve the problem        bool exist = false;        vector<int> a(n);        for (int i = 0; i < n; i++) {            cin >> a[i];            if (a[i] == k) {                exist = true;            }        }        if (exist) {            cout << "YES\n";        } else {            cout << "NO\n";        }    }    return 0;}